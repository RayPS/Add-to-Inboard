{
  "name": "Add to Inboard",
  "description": "A Browser Extension for Inboard App",
  "version": "1.2",
  "permissions": [
    "activeTab",
    "*://*.dribbble.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.dribbble.com/*"],
      "css": ["inject.css"],
      "js": ["inject.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "*.png"
  ],
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "manifest_version": 2
}
